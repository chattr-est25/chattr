{
  "dependencies": {
    "@elysiajs/openapi": "catalog:",
    "@t3-oss/env-core": "catalog:",
    "elysia": "catalog:",
    "lib": "workspace:*",
    "zod": "catalog:"
  },
  "devDependencies": {
    "config": "workspace:*"
  },
  "module": "src/index.ts",
  "name": "gateway",
  "private": true,
  "scripts": {
    "build": "bun build --compile --minify-whitespace --minify-syntax --target bun-linux-x64 --outfile server src/index.ts",
    "dev": "bun run --watch src/server.ts",
    "start": "./server",
    "typecheck": "tsc --noEmit"
  },
  "type": "module"
}
